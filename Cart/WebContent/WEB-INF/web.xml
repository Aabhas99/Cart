<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">



<servlet>
	<servlet-name>
		create_order
	</servlet-name>
	<servlet-class>
			order.Create_order
	</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>
		create_order
	</servlet-name>
	<url-pattern>
		/create_order
	</url-pattern>
</servlet-mapping>



<servlet>
	<servlet-name>
		order_done
	</servlet-name>
	<servlet-class>
			order.Order_done
	</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>
		order_done
	</servlet-name>
	<url-pattern>
		/order_done
	</url-pattern>
</servlet-mapping>


<servlet>
	<servlet-name>
		logout
	</servlet-name>
	<servlet-class>
			order.Logout
	</servlet-class>
</servlet>
<servlet-mapping>
	<servlet-name>
		logout
	</servlet-name>
	<url-pattern>
		/logout
	</url-pattern>
</servlet-mapping>
</web-app>